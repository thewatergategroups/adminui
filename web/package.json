        "@mantine/form": "^7.8.1",
        "@mantine/hooks": "^7.8.1",
        "@mui/material": "^5.14.18",
        "@tabler/icons-react": "^3.2.0",
        "@tanstack/react-query": "^5.32.0",
        "@types/js-cookie": "^3.0.6",
        "@types/react-router-dom": "^5.3.3",
        "axios": "^1.6.2",
        "js-cookie": "^3.0.5",
        "lottie-react": "^2.4.0",
        "prettier": "^3.2.5",
        "react": "^18.2.0",
